{
  "name": "punch-me",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "npm run load:ip && expo run:android",
    "check:all": "npm run lint && npm run type:check",
    "devtools": "npx react-devtools",
    "gen:types": "npx supabase gen types typescript --project-id jsoywqogrthynrvuduco > supabase/generated-types.ts && npm run zodify",
    "ios": "expo run:ios",
    "lint": "expo lint",
    "load:ip": "node scripts/load-ip.js",
    "npm:clean": "rm -rf node_modules && npm install",
    "prebuild": "npx expo prebuild",
    "prebuild:c": "npx expo prebuild --clean",
    "react:devtools": "npx react-devtools",
    "reset-project": "node ./scripts/reset-project.js",
    "start": "npm run load:ip && expo start",
    "start:c": "npm run start -- -c",
    "test": "jest --watchAll",
    "type:check": "tsc --noEmit",
    "web": "expo start --web",
    "zodify": "npx supabase-to-zod --input supabase/generated-types.ts --output supabase/zod-types.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@dev-plugins/react-query": "^0.0.7",
    "@expo/vector-icons": "^14.0.2",
    "@hookform/resolvers": "^3.9.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-navigation/native": "^6.0.2",
    "@supabase/supabase-js": "^2.45.2",
    "@tanstack/react-query": "^5.52.0",
    "axios": "^1.7.4",
    "clsx": "^2.1.1",
    "expo": "~51.0.28",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.23",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-location": "~17.0.1",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "i18next": "^23.14.0",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.1",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-qrcode-svg": "^6.3.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.6.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.10",
    "tailwind-merge": "^2.5.2",
    "ts-to-zod": "^3.13.0",
    "uninstall": "^0.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tanstack/eslint-plugin-query": "^5.56.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "@vercel/style-guide": "^6.0.0",
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.1.7",
    "reactotron-react-query": "^1.0.4",
    "supabase": "^1.191.3",
    "supabase-to-zod": "^1.0.7",
    "tailwindcss": "^3.3.2",
    "type-fest": "^4.26.0",
    "typescript": "~5.3.3"
  }
}
